{
  "name": "stylelint-browser-compat",
  "version": "1.0.0-beta.55",
  "description": "Yet another linter rule to detect compatibility of CSS features.",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "linter",
    "compatibility"
  ],
  "funding": "https://github.com/sponsors/3846masa",
  "license": "MIT",
  "author": "3846masa <3846masahiro+git@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!**/__tests__"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib --extensions .js,.ts --ignore **/*.d.ts",
    "pretest": "npm run build",
    "test": "jest",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.mjs,.mts .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "format": "npm-run-all -c format:eslint format:prettier",
    "format:eslint": "eslint --fix --ext .js,.ts,.mjs,.mts .",
    "format:prettier": "prettier --write .",
    "semantic-release": "semantic-release",
    "patch-package": "patch-package"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "5.2.48",
    "array.prototype.at": "1.1.1",
    "lodash.get": "4.4.2",
    "postcss-media-query-parser": "0.2.3",
    "postcss-selector-parser": "6.0.11",
    "postcss-value-parser": "4.2.0",
    "semver": "7.3.8",
    "string.prototype.at": "1.0.4",
    "ts-results": "3.3.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@3846masa/configs": "github:3846masa/configs#a0b859ba9a6a354f7c2d3433110d87d4874833c6",
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/common-tags": "1.8.1",
    "@types/fs-extra": "11.0.1",
    "@types/lodash.get": "4.4.7",
    "@types/semver": "7.3.13",
    "babel-jest": "29.5.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-polyfill-es-shims": "0.8.0",
    "browserslist": "4.21.5",
    "common-tags": "1.8.2",
    "fs-extra": "11.1.1",
    "jest": "29.5.0",
    "jest-preset-stylelint": "6.1.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.5.1",
    "rimraf": "4.4.1",
    "semantic-release": "19.0.5",
    "stylelint": "15.4.0",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "browserslist": ">=4.20.3",
    "stylelint": ">=14.8.5"
  }
}
