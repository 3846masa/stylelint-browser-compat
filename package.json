{
  "name": "stylelint-browser-compat",
  "version": "1.0.0-beta.4",
  "description": "Yet another linter rule to detect compatibility of CSS features.",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "linter",
    "compatibility"
  ],
  "license": "MIT",
  "author": "3846masa <3846masahiro+git@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!**/__tests__"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib --extensions .js,.ts --ignore **/*.d.ts",
    "pretest": "npm run build",
    "test": "jest",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.mjs,.mts .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "format": "npm-run-all -c format:eslint format:prettier",
    "format:eslint": "eslint --fix --ext .js,.ts,.mjs,.mts .",
    "format:prettier": "prettier --write .",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "5.1.2",
    "dot-prop": "6.0.1",
    "postcss-media-query-parser": "0.2.3",
    "postcss-selector-parser": "6.0.10",
    "postcss-value-parser": "4.2.0",
    "semver": "7.3.7",
    "ts-results": "3.3.0",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@3846masa/configs": "github:3846masa/configs#c80e1fee19f90246b2d5e1bc8d5843c9001c5c8c",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/common-tags": "1.8.1",
    "@types/fs-extra": "9.0.13",
    "@types/semver": "7.3.7",
    "babel-jest": "28.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-polyfill-es-shims": "0.6.1",
    "browserslist": "4.21.0",
    "common-tags": "1.8.2",
    "fs-extra": "10.1.0",
    "jest": "28.1.0",
    "jest-preset-stylelint": "5.0.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.3",
    "stylelint": "14.8.5",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "browserslist": ">=4.20.3",
    "stylelint": ">=14.8.5"
  }
}
